<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>AsmodatStandard</AssemblyName>
    <RootNamespace>AsmodatStandard</RootNamespace>
    <Platforms>AnyCPU;x64</Platforms>
    <Product>AsmodatStandard</Product>
    <Description>Multipurpose .NET Standard Extensions</Description>
    <PackageId>Asmodat.Standard</PackageId>
    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
    <PackageReleaseNotes>2018-07-10</PackageReleaseNotes>
    <Version>1.0.0.5</Version>
    <PackageProjectUrl>https://github.com/asmodat/Asmodat-Standard</PackageProjectUrl>
    <PackageTags>Asmodat Standard, Asmodat, AsmodatStandard, Extensions, Standard</PackageTags>
    <PackageRequireLicenseAcceptance>False</PackageRequireLicenseAcceptance>
    <Company>AsmodatStandard</Company>
    <PackageIconUrl>https://avatars3.githubusercontent.com/u/10515811</PackageIconUrl>
    <Authors>AsmodatStandard</Authors>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Win32.Registry" Version="4.4.0" />
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
    <PackageReference Include="protobuf-net" Version="2.3.3" />
    <PackageReference Include="System.IO.Compression.ZipFile" Version="4.3.0" />
    <PackageReference Include="System.Threading.AccessControl" Version="4.4.0" />
    <PackageReference Include="System.Threading.Tasks.Dataflow" Version="4.8.0" />
  </ItemGroup>

</Project>
